    "import re as re\n",
    "# read en.txt file \n",
    "with open(\"en.txt\", \"r\") as f:\n",
    "    en = f.read()\n",
    "    #replace ,\\n or .\\n with # \n",
    "    en = re.sub(r\"\\n\", ',\\n', en)\n",
    "  \n",
    "with open(\"en.txt\", \"w\") as f:\n",
    "    f.write(en)\n"
   ]
  }
